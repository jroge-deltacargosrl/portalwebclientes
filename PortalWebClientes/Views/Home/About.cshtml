@{ViewBag.Title = "About";}
@using System.Collections.Generic;
@using PortalWebClientes.Controllers;
@model List<User>

<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>
<h4>Su email es: @ViewBag.email</h4>

<p>Use this area to provide additional information.</p>
<table>
    <thead> 
        <tr>
            <td>
                Email
            </td>
            <td>
                Nombre
            </td> 
        </tr>
    </thead>
    <tbody>
        @{
            foreach (User user in Model)
            {
                <tr>
                    <td>
                        @user.email
                    </td>
                    <td>
                        @user.name
                    </td>
                </tr>
            }
        }
    </tbody>
</table>